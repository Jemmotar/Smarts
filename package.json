{
  "name": "smarts",
  "version": "1.2.1",
  "description": "Evaluation simulator for Smarts Network Filters solution",
  "main": "main.bundle.js",
  "scripts": {
    "dev": "set START_MAIN=true&& npm run dev-renderer",
    "dev-main": "set NODE_ENV=development&& electron -r babel-register ./src/main/main.js",
    "dev-renderer": "webpack-dev-server --env.target=renderer --env.dev --progress --config ./config/webpack/config.handler.js",
    "build": "npm run build-main & npm run build-renderer",
    "build-main": "webpack --env.target=main --env.prod --progress --config ./config/webpack/config.handler.js",
    "build-renderer": "webpack --env.target=renderer --env.prod --progress --config ./config/webpack/config.handler.js"
  },
  "keywords": [
    "electron",
    "react",
    "redux"
  ],
  "author": "Filipsi,Jemmotar",
  "license": "GPL-3.0",
  "dependencies": {
    "node-watch": "^0.5.5",
    "parse-json": "^4.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-catch": "^1.3.1",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.76.0",
    "source-map-support": "^0.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-private-underscore": "^0.1.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "copy-webpack-plugin": "^4.3.0",
    "css-loader": "^0.28.11",
    "electron": "^1.8.4",
    "electron-devtools-installer": "^2.2.1",
    "eslint": "^4.18.1",
    "eslint-config-happiness": "^10.2.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.11",
    "react-hot-loader": "^4.0.0",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "webpack-electron-packager": "^1.0.0"
  }
}
